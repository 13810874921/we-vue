{
  "name": "we-vue",
  "version": "1.5.3",
  "description": "Vue components based on Vue2.x and weui1.x",
  "keywords": [
    "vue",
    "weui",
    "we-vue",
    "component"
  ],
  "author": "tianyong90 <412039588@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tianyong90/we-vue.git"
