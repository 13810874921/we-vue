  "main": "lib/index.js",
  "style": "lib/style.css",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "node build/build.js",
    "build:component": "node build/build-component.js",
    "build:demo": "node build/build-demo.js",
    "build:entry": "node build/build-entry.js",
    "build:release": "npm run build:entry && npm run build:component && npm run build",
    "dev": "npm run build:entry && webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "lint": "eslint --ext .js,.vue src example test/unit/specs",
    "start": "npm run dev",
    "test": "npm run unit",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "pub": "node build/release.js"
  },
  "dependencies": {
    "vue-lazyload": "^1.1.3",
    "weui": "^1.1.2"
  },
